   "source": [
    "import math\n",
    "def feature_mask_list(num, size):\n",
    "    mask = []\n",
    "    while size!=0:\n",
    "        mask.append(num%2 ==1)\n",
    "        num = math.floor(num/2)\n",
    "        size = math.floor(size/2)\n",
    "    return mask"
   ]
